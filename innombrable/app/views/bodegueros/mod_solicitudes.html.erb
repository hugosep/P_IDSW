<h1>Ver/Modificar soliticitudes</h1>
<table style="width:90%">
  <tr>
    <th>Material</th>
    <th>Encargado</th>
    <th>Cantidad</th>
    <th>Prioridad</th>
    <th>Estado</th>
    <th>Modificar</th>
  </tr>
  <% @solicitudes.each do |s| %>
  <tr>
    <td><%= s.material %></td>
    <td><%= s.encargado_id %></td>
    <td><%= s.cantidad %></td>
    <td><%= s.prioridad %></td>
    <td><%= s.estado %></td>
    <td><%= s.submit "Modificar" class: "button" %></td>
  </tr>
  <% end %>
</table>
