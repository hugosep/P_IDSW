<%= form_for(:solicitude, url:{:action=>'create'}) do |s| %>

<%= s.label :Material %>
<%= s.text_field :material, class: 'form-control' %>

<%= s.label :Cantidad %>
<%= s.number_field :cantidad, class: 'form-control' %>

<%= s.label :Prioridad %>
<%= s.number_field :prioridad, class: 'form-control' %>

<%= s.submit "Crear solicitud", class: "btn btn-primary" %>

<% end %>
